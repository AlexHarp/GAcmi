]]></value><format>basic_html</format></field_overview><field_principal_contact><nid><value>65</value></nid><uuid><value>0902e4b7-2788-46e6-ac6e-603aba2d98e9</value></uuid><vid><value>113</value></vid><langcode><value>en</value></langcode><type><target_id>contact</target_id><target_type>node_type</target_type><target_uuid>e3672a17-5d33-40f0-b4f8-0249345d8c91</target_uuid></type><status><value>1</value></status><title><value>Geoscience Australia - Client Services</value></title><uid><target_id>1</target_id><target_type>user</target_type><target_uuid>2c1b7b76-12fb-4273-9255-773773f2ddb6</target_uuid><url>/user/1</url></uid><created><value>1488412643</value></created><changed><value>1488412736</value></changed><promote><value>0</value></promote><sticky><value>0</value></sticky><revision_timestamp><value>1488412736</value></revision_timestamp><revision_uid><target_id>1</target_id><target_type>user</target_type><target_uuid>2c1b7b76-12fb-4273-9255-773773f2ddb6</target_uuid><url>/user/1</url></revision_uid><revision_log/><revision_translation_affected><value>1</value></revision_translation_affected><default_langcode><value>1</value></default_langcode><moderation_state/><workspace><target_id>1</target_id><target_type>workspace</target_type><target_uuid>79bd7389-c9e5-40f5-ace7-5c0ecc466e6c</target_uuid></workspace><_deleted><value>0</value></_deleted><_rev><value>1-4fdce2632031ff6de1e421c5d6380ab2</value></_rev><path/><field_contact_name/><field_email><value>clientservices@ga.gov.au</value></field_email><field_organization><nid><value>3</value></nid><uuid><value>2ac5309e-a20c-4c29-b1b0-e2dba4d17f90</value></uuid><vid><value>3</value></vid><langcode><value>en</value></langcode><type><target_id>organization</target_id><target_type>node_type</target_type><target_uuid>b16be7e4-0b38-4918-9d08-6bd8b864b0f8</target_uuid></type><status><value>1</value></status><title><value>Geoscience Australia</value></title><uid><target_id>1</target_id><target_type>user</target_type><target_uuid>